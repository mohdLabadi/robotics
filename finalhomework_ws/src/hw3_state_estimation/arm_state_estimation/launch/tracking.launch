<launch>
  <arg name="plot" default="False"/>

  <include file="$(find arm_state_estimation)/launch/env.launch"/>
  <node name="visualize_cylinder" pkg="arm_state_estimation" type="visualize_cylinder.py"/>

  <node name="object_tracking" pkg="arm_state_estimation" type="object_tracking.py" output="screen">
    <param name="plot" value="$(arg plot)"/>
  </node>

</launch>
